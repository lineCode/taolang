function main() {
    for1();
    for2();
    for3();
}

function for1() {
    for {
        println("for1");
        break;
    }
}

function for2() {
    let n = 3;
    for n >= 0 {
        println(n);
        n = n - 1;
    }
}

function for3() {
    for let n = 3; n >= 0; n = n - 1 {
        println(n);
    }
}
